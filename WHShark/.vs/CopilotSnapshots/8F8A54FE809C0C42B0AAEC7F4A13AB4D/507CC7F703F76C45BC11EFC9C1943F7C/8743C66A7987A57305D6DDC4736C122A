using System;
using System.Windows.Forms;

namespace WHUI.Login
{
    public partial class LogIn : Form
    {
        public LogIn()
        {
            InitializeComponent();
        }

        private void btnLogin_Click(object sender, EventArgs e)
        {
            // Aquí iría la validación de credenciales. Por ahora asumimos éxito.
            // Si la validación es correcta, establecer DialogResult y cerrar el formulario.
            this.DialogResult = DialogResult.OK;
            this.Close();
        }
    }
}